#!/usr/bin/env bash

# tlvm: Triste-le-Van-Machine
# a little wrapper around the calls to Racket needed
# to run files produced by compass -v

usage() {
cat <<EOF
Usage: tlvm FILE
Run .obj files generated by compass -v
FILE must be present in the current directory
Example: tlvm fib.obj

EOF
}

if [ $# -lt 1 ]
then
    usage
fi

srcPath="/home/argv/Documents/projects/lonnrot/src"

# TODO:
# racket -t ${srcPath}/vm/vm.rkt -m $1
